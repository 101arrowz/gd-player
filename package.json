{
  "name": "gd-player",
  "version": "1.0.0",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.14.2",
    "@types/plist": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "parcel": "^2.0.0-nightly.433",
    "parcel-config-precache-manifest": "^0.0.2",
    "parcel-config-pwa-manifest": "^0.0.8",
    "plist": "^3.0.1",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "start": "yarn build:assets && parcel serve src/index.html",
    "lint": "eslint --fix \"src/**/*.ts\" \"scripts/*.ts\"",
    "sc": "node -r ts-node/register scripts/$SC.ts",
    "build:assets": "SC=copyAssets yarn sc",
    "build": "yarn build:assets && parcel build src/index.html"
  },
  "dependencies": {
    "fflate": "^0.3.1",
    "gd.js": "^0.2.7",
    "pixi.js": "^5.3.3",
    "preact": "^10.5.5"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "pixi.js": "./src/pixi.js"
  }
}
